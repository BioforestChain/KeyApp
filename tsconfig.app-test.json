{
    "extends": "./tsconfig.app.json",
    "compilerOptions": {
        "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.app-test.tsbuildinfo",
        /* Test/Stories files 放宽严格检查 */
        "noUnusedLocals": false,
        "noUnusedParameters": false
    },
    "include": [
        "src/**/*.test.ts",
        "src/**/*.test.tsx",
        "src/**/*.stories.ts",
        "src/**/*.stories.tsx",
        "src/test/**/*"
    ]
}