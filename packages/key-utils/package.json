{
  "name": "@biochain/key-utils",
  "private": true,
  "version": "0.1.0",
  "description": "Utility functions for KeyApp UI components",
  "type": "module",
  "main": "./src/index.ts",
  "module": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "import": "./src/index.ts",
      "types": "./src/index.ts"
    },
    "./*": {
      "import": "./src/*.ts",
      "types": "./src/*.ts"
    }
  },
  "files": [
    "src"
  ],
  "scripts": {
    "build": "vite build",
    "dev": "vite build --watch",
    "typecheck": "tsc --noEmit",
    "typecheck:run": "tsc --noEmit",
    "test": "vitest",
    "test:run": "vitest run --passWithNoTests",
    "test:storybook": "echo 'No storybook'",
    "lint:run": "oxlint .",
    "i18n:run": "echo 'No i18n'",
    "theme:run": "echo 'No theme'"
  },
  "peerDependencies": {
    "react": "^19.0.0"
  },
  "devDependencies": {
    "@types/react": "^19.0.0",
    "jsdom": "^26.1.0",
    "oxlint": "^1.32.0",
    "typescript": "^5.9.3",
    "vite": "^7.3.0",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^4.0.0"
  },
  "keywords": [
    "biochain",
    "keyapp",
    "utils"
  ],
  "license": "MIT"
}
