{
  "name": "@biochain/android-focus-blur-guard",
  "private": false,
  "version": "0.1.0",
  "description": "Android focus/blur loop guard for WebView and iframe contexts",
  "type": "module",
  "main": "./src/index.ts",
  "module": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "import": "./src/index.ts",
      "types": "./src/index.ts",
